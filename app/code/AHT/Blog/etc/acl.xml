<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="AHT_Blog::Blog" title="Blog" sortOrder="10">
					<resource id="AHT_Blog::Blog_save" title="Save Blog" sortOrder="10"/>
					<resource id="AHT_Blog::Blog_delete" title="Delete Blog" sortOrder="20"/>
					<resource id="AHT_Blog::Blog_update" title="Update Blog" sortOrder="30"/>
					<resource id="AHT_Blog::Blog_view" title="View Blog" sortOrder="40"/>
				</resource>
				<resource id="AHT_Blog::Category" title="Category" sortOrder="10">
					<resource id="AHT_Blog::Category_save" title="Save Category" sortOrder="10"/>
					<resource id="AHT_Blog::Category_delete" title="Delete Category" sortOrder="20"/>
					<resource id="AHT_Blog::Category_update" title="Update Category" sortOrder="30"/>
					<resource id="AHT_Blog::Category_view" title="View Category" sortOrder="40"/>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
